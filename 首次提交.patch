+        }
+    }
+
+    fun setOnItemClickListener(listener: OnItemClickListener){
+        this.onItemClickListener = listener
+    }
+
+    interface OnItemClickListener{
+        fun onItemClick(item: NavigationAdapter.Items)
+    }
